    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@types/color": "^3.0.2",
    "@types/react": "^17.0.3",
    "@vanilla-extract/css": "^1.1.0",
    "@vanilla-extract/esbuild-plugin": "^1.1.0",
    "babel-loader": "^8.2.2",
    "color": "^3.1.3",
