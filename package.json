{
  "name": "react-haze",
  "version": "0.0.0",
  "description": "Easily show a fade at edges of lists to indicate scrollability",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvarlagerlof/react-haze"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typescript": "tsc --noEmit",
    "typescript-watch": "tsc --noEmit --watch",
    "esbuild": "node build.js",
    "esbuild-watch": "node build.js watch",
    "build": "yarn run typescript && yarn run esbuild",
    "build-watch": "concurrently \"yarn run typescript-watch\" \"yarn run esbuild-watch\""
  },
  "keywords": [
    "react",
    "list",
    "scroll",
    "responsive",
    "haze",
    "fog",
    "mist"
  ],
  "author": "Alvar Lagerl√∂f <hello@alvar.dev> (https://alvar.dev)",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.3",
    "concurrently": "^6.0.1",
    "esbuild": "^0.11.7",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
