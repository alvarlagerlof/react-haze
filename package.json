{
  "name": "react-haze",
  "version": "0.0.3",
  "description": "Easily show a fade at edges of lists to indicate scrollability",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvarlagerlof/react-haze"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typescript": "tsc --noEmit",
    "typescript-watch": "tsc --noEmit --watch",
    "typescriptOLD": "tsc --emitDeclarationOnly",
    "typescript-watchOLD": "tsc --emitDeclarationOnly --watch",
    "esbuild": "node build.js",
    "esbuild-watch": "node build.js watch",
    "build": "yarn run typescript && yarn run esbuild",
    "build-watch": "concurrently \"yarn run typescript-watch\" \"yarn run esbuild-watch\"",
    "storybook": "start-storybook -p 6006 ",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "list",
    "scroll",
    "responsive",
    "haze",
    "fog",
    "mist"
  ],
